{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "BonsaiList",
  "type": "object",
  "required": [
    "bonsais"
  ],
  "properties": {
    "bonsais": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/Bonsai"
      }
    }
  },
  "definitions": {
    "Bonsai": {
      "type": "object",
      "required": [
        "birth_date",
        "id",
        "price"
      ],
      "properties": {
        "birth_date": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        },
        "id": {
          "type": "string"
        },
        "price": {
          "$ref": "#/definitions/Coin"
        }
      }
    },
    "Coin": {
      "type": "object",
      "required": [
        "amount",
        "denom"
      ],
      "properties": {
        "amount": {
          "$ref": "#/definitions/Uint128"
        },
        "denom": {
          "type": "string"
        }
      }
    },
    "Uint128": {
      "type": "string"
    }
  }
}

{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "AllGardenersResponse",
  "type": "object",
  "required": [
    "gardeners"
  ],
  "properties": {
    "gardeners": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/Gardener"
      }
    }
  },
  "definitions": {
    "Binary": {
      "description": "Binary is a wrapper around Vec<u8> to add base64 de/serialization with serde. It also adds some helper methods to help encode inline.\n\nThis is only needed as serde-json-{core,wasm} has a horrible encoding for Vec<u8>",
      "type": "string"
    },
    "Bonsai": {
      "type": "object",
      "required": [
        "birth_date",
        "id",
        "price"
      ],
      "properties": {
        "birth_date": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        },
        "id": {
          "type": "string"
        },
        "price": {
          "$ref": "#/definitions/Coin"
        }
      }
    },
    "CanonicalAddr": {
      "$ref": "#/definitions/Binary"
    },
    "Coin": {
      "type": "object",
      "required": [
        "amount",
        "denom"
      ],
      "properties": {
        "amount": {
          "$ref": "#/definitions/Uint128"
        },
        "denom": {
          "type": "string"
        }
      }
    },
    "Gardener": {
      "type": "object",
      "required": [
        "address",
        "bonsais",
        "name"
      ],
      "properties": {
        "address": {
          "$ref": "#/definitions/CanonicalAddr"
        },
        "bonsais": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Bonsai"
          }
        },
        "name": {
          "type": "string"
        }
      }
    },
    "Uint128": {
      "type": "string"
    }
  }
}
